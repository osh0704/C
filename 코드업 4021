
#include <stdio.h>
 
int main(){
    int num[7];
    int sum = 0;
    
    for(int i=0; i<sizeof(num)/sizeof(int); i++){
        scanf("%d", &num[i]);
        if(num[i] % 2 == 1) {
            sum += num[i];
        }
    }
    
    if(sum == 0) { 
        printf("%d\n", -1);
    }
    else {
        printf("%d\n", sum);
    }
    
    return 0;
}
