#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main()
{
	int n, m, sum = 0, arr[1000] = {0};
	int cnt = 0, ccnt = 1;
	scanf("%d %d", &n, &m);
	for (int i = 0; i < 1000; i++) {
		arr[i] = ccnt;
		cnt++;
		if (ccnt == cnt) {
			ccnt++;
			cnt = 0;
		}
	}
	for (int i = n - 1; i <= m - 1; i++) {
		sum += arr[i];
	}
	printf("%d", sum);
}
