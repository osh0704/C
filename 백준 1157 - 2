#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main()
{
	char word[1000000];
	int cnt[150] = { 0 }, max = 0, tmp = 0, sam = 0;
	scanf("%[^\n]", word);
	int len = strlen(word);
	for (int i = 0; i < len ; i++) cnt[word[i]]++;
	for (int i = 97; i <= 122; i++) cnt[i - 32] += cnt[i];
	for (int i = 65; i < 97; i++) {
		if (cnt[i] == max) sam = max;
		if (cnt[i] > max) {
			max = cnt[i];
			tmp = i;
		}
	}

	if (max == sam) printf("?");
	else printf("%c", tmp);

}
